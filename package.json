{
  "name": "angular-forms-extension",
  "version": "0.1.28",
  "description": "A form library empowering Angular's template driven forms",
  "main": "./bundles/quickstart-lib.umd.js",
  "module": "./angular-forms-extension.es5.js",
  "es2015": "./angular-forms-extension.js",
  "typings": "./angular-forms-extension.d.ts",
  "author": "",
  "license": "MIT",
  "repository": "github:sapienstech/angular-forms-extension",
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "clean": "rimraf out-tsc dist/*",
    "prebuild": "npm run clean",
    "build": "node build.js",
    "build-demo": "tsc -p src/demo/",
    "build-demo:watch": "tsc -p src/demo/ -w",
    "serve": "lite-server -c=bs-config.json",
    "prestart": "npm run build-demo",
    "start": "concurrently \"npm run build-demo:watch\" \"npm run serve\"",
    "build-test": "tsc -p src/lib/tsconfig.spec.json",
    "build-test:watch": "tsc -p src/lib/tsconfig.spec.json -w",
    "pretest": "npm run build-test",
    "test": "npm run build-test && karma start karma.conf.js --single-run",
    "test:watch": "concurrently \"npm run build-test:watch\" \"karma start karma.conf.js\"",
    "pretest:once": "npm run build-test",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "release": "standard-version && npm run build && git push --follow-tags origin master && npm publish ./dist"
  },
  "peerDependencies": {
    "@angular/core": ">=4.0.0 <5.0.0 || >=4.0.0-beta <5.0.0"
  },
  "devDependencies": {
    "@angular/common": "~4.3.6",
    "@angular/compiler": "~4.3.6",
    "@angular/compiler-cli": "~4.3.6",
    "@angular/core": "~5.1.3",
    "@angular/forms": "~4.3.6",
    "@angular/platform-browser": "~4.3.6",
    "@angular/platform-browser-dynamic": "~4.3.6",
    "@types/jasmine": "2.5.54",
    "@types/node": "^8.0.26",
    "camelcase": "^4.0.0",
    "concurrently": "3.5.0",
    "core-js": "^2.4.1",
    "glob": "^7.1.1",
    "jasmine-core": "^2.5.2",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-html-reporter": "^0.2.7",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "^1.2.0",
    "lite-server": "^2.2.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.42.0",
    "rollup-plugin-sourcemaps": "^0.4.1",
    "rollup-plugin-uglify": "^2.0.1",
    "rxjs": "^5.0.1",
    "standard-version": "^4.0.0",
    "systemjs": "^0.20.18",
    "tslint": "^5.7.0",
    "typescript": "^2.5.1",
    "zone.js": "^0.8.4"
  }
}
